{
  "extends": ["next/core-web-vitals", "@rocketseat/eslint-config/react"],
  "rules": {
    "prettier/prettier": [
      "error",
      {
        "semi": false,
        "singleQuote": true,
        "arrowParens": "avoid",
        "trailingComma": "none",
        "endOfLine": "auto",
        "tabWidth": 2,
        "printWidth": 120
      }
    ]
  }
}
